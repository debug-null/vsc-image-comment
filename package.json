{
	"name": "image-comment",
	"displayName": "image-comment",
	"description": "Add image comments to any of your code",
	"version": "0.0.1",
	"publisher": "pseudo",
	"engines": {
		"vscode": "^1.67.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/debug-null/vsc-image-comment.git"
	},
	"icon": "images/icon.png",
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [],
		"keybindings": [
			{
				"command": "will.selection",
				"key": "alt+z"
			}
		],
		"configuration": {
			"type": "object",
			"title": "willPasteImg",
			"properties": {
				"will.imgSavePath": {
					"type": "string",
					"default": "rgba(255, 255, 0, 0.05)",
					"description": "%will.imgSavePath%"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "14.x",
		"@types/vscode": "^1.67.0",
		"@typescript-eslint/eslint-plugin": "^5.21.0",
		"@typescript-eslint/parser": "^5.21.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.14.0",
		"glob": "^8.0.1",
		"mocha": "^9.2.2",
		"ts-loader": "^9.2.8",
		"typescript": "^4.6.4",
		"webpack": "^5.70.0",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"fs-extra": "^10.1.0"
	}
}